<!DOCTYPE html>
<html>
<head>
</head>

<body>
<h1>Friend Requests Sent To: </h1>

<ul>
  <table class="tbl">
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
   <% rows.forEach(function(r) { %>
      <tr>
        <td><%= r.name %></td>
        <td><%= r.email %></td>
      </tr>
    <% }); %>
  </table>

</ul>

</body>
</html>
