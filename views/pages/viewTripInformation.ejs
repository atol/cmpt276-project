  <title>Ditu - <%= uname %></title>
</head>

<body>
  <div id="mySidepanel" class="sidepanel">
    <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
    <img src="../image/defaultdp.png" width="50px" height="50px">
    <p id="Username"><%= uname %></p></br>
    <button type="button" onclick="popup()" class="btn btn-primary options" data-toggle="modal"
      data-target="#exampleModal">
      Create New Trip
    </button>
    <a href="#"><button class="options">View Trip Itinerary</button></a><br>
    <a href="#"><button class="options">View Trip Information</button></a><br>
    <a href="/logout"><button class="options">Log Out</button></a>
  </div>


  <button class="openbtn" onclick="openNav()">&#9776; Menu</button>

  <!-- Trigger the modal with a button -->


  <!-- Modal -->


  <table class="table">
    <thead>
      <tr>
        <th>Identification</th>
        <th>Name</th>
        <th>Age</th>
        <th>Height</th>
        <th>Weight</th>
      </tr>
    </thead>
    <tbody>
      <% for (var i = 0; i < rows.length; i++){ %>
      <tr>
        <td><%=rows[i].id %></td>
        <td><%=rows[i].name %></td>
        <td><%=rows[i].age %></td>
        <td><%=rows[i].height %></td>
        <td><%=rows[i].weight %></td>
      </tr>
      <%}%>
        </tbody>
      </table>
  <iframe id="map"
    src='https://api.mapbox.com/styles/v1/keitolee/ckc8n54gm12sr1ip8xlk5yv3q.html?fresh=true&title=false&access_token=pk.eyJ1Ijoia2VpdG9sZWUiLCJhIjoiY2tjOG0wbXhuMGN1ZTM0cGZ1d28zcWx6cyJ9.oSR2bnxW0qtkQ3YM8pBRZg'>
</body>

