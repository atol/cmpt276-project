<!DOCTYPE html>
<html>
<head>
</head>

<body>
<h4>Friend Requests Recieved From </h4>

<ul>
  <table class="tbl">
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
   <% rows.forEach(function(r) { %>
      <tr>
        <td><%= r.name %></td>
        <td><%= r.email %></td>
        <button >Accept</button>
        <button>Decline</button>
      </tr>
    <% }); %>
  </table>

</ul>

</body>
</html>
